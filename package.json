{
  "name": "@salesforce/sfdx-plugin-lwc-test",
  "description": "Tools for unit testing Lightning web components in a Salesforce DX workspace",
  "version": "0.1.1",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/errors": "^1.2.2",
    "@salesforce/command": "^3.0.0",
    "@salesforce/core": "^2.2.0",
    "@salesforce/kit": "^1.2.1",
    "semver-compare": "^1.0.0",
    "signal-exit": "^3.0.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/plugin-command-snapshot": "^1.0.0",
    "@oclif/plugin-help": "^1",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.1.0",
    "@salesforce/plugin-command-reference": "^1.0.6",
    "@salesforce/ts-sinon": "^1.1.1",
    "@salesforce/ts-types": "^1.2.1",
    "@types/chai": "4",
    "@types/jsforce": "^1.9.13",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "chai": "^4",
    "globby": "^8",
    "isbinaryfile": "^3.0.3",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "sfdx",
    "commands": "./lib/commands",
    "topics": {
      "force": {
        "external": true,
        "subtopics": {
          "lightning": {
            "external": true,
            "subtopics": {
              "lwc": {
                "description": "tools for Lightning web components in a Salesforce DX workspace",
                "longDescription": "Tools for Lightning web components in a Salesforce DX workspace.",
                "subtopics": {
                  "test": {
                    "description": "tools for unit testing Lightning web components in a Salesforce DX workspace",
                    "longDescription": "Tools for unit testing Lightning web components in a Salesforce DX workspace."
                  }
                }
              }
            }
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ]
  },
  "repository": "salesforce/sfdx-plugin-lwc-test",
  "scripts": {
    "check-license-headers": "node ./scripts/checkLicenseHeaders.js",
    "build": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish && yarn test:deprecation && yarn test:commandreference",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "test:deprecation": "./bin/run snapshot:compare",
    "test:commandreference": "./bin/run commandreference:generate -p @salesforce/sfdx-plugin-lwc-test --erroronwarnings",
    "version": "oclif-dev readme && git add README.md"
  }
}
